<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsTextColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="pstsTextSize" format="float" />
        <attr name="pstsSelectTextSize" format="float" />
        <attr name="selectedTabTextColor" format="reference" />
        <attr name="pstsIndicatiorWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ItemLayout">
        <attr name="leftIcon" format="reference"></attr>
        <attr name="rightIcon" format="reference"></attr>
        <attr name="leftTitleTxt" format="string"></attr>
        <attr name="rightTitleTxt" format="string"></attr>
        <attr name="leftTitleTextColor" format="color"></attr>
        <attr name="rightTitleTextColor" format="color"></attr>
        <attr name="leftTitleTextSize" format="float"></attr>
        <attr name="rightTitleTextSize" format="float"></attr>
    </declare-styleable>


    <declare-styleable name="RatioImageView">
        <attr name="ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="NineGridLayout">
        <attr name="sapcing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SampleHeaderView">
        <attr name="shv_title" format="string" />
        <attr name="shv_expandedColor" format="color" />
        <attr name="shv_collapsedColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SectorMenuButton">
        <attr name="aebMainButtonSizeDp" format="dimension|reference" />
        <attr name="aebSubButtonSizeDp" format="dimension|reference" />
        <attr name="aebButtonGapDp" format="dimension|reference" />
        <attr name="aebStartAngleDegree" format="integer|reference" />
        <attr name="aebEndAngleDegree" format="integer|reference" />
        <attr name="aebAnimDurationMillis" format="integer|reference" />
        <attr name="aebButtonElevation" format="dimension|reference" />
        <attr name="aebMaskBackgroundColor" format="color|reference" />
        <attr name="aebIsSelectionMode" format="boolean" />
        <attr name="aebMainButtonTextColor" format="color|reference" />
        <attr name="aebSubButtonTextColor" format="color|reference" />
        <attr name="aebMainButtonTextSizeSp" format="dimension|reference" />
        <attr name="aebSubButtonTextSizeSp" format="dimension|reference" />
        <attr name="aebRippleEffect" format="boolean" />
        <attr name="aebRippleColor" format="color|reference" />
        <attr name="aebMainButtonRotateDegree" format="integer|reference" />
        <attr name="aebMainButtonRotateAnimDurationMillis" format="integer|reference" />
        <attr name="aebBlurBackground" format="boolean" />
        <attr name="aebBlurRadius" format="float|reference" />
    </declare-styleable>
</resources>